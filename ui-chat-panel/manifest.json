{
  "manifest_version": 3,
  "name": "Chat Navigation Extension",
  "version": "1.0",
  "description": "Adds a chat-like side panel for URL navigation",
  "permissions": ["tabs", "sidePanel", "storage"],
  "action": {
    "default_icon": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  },
  "side_panel": {
    "default_path": "panel.html"
  }
}